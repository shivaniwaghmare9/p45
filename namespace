#include <iostream>
using namespace std;


    namespace cybrom {
        void student(){
            cout<<"\n cybrom student\n";
        }
     namespace bhopal{
         void student(){
             cout<<"\n bhopal studnet\n";
         }
     } 
     void show(){
         cout<<"\n first program\n";
     }
     class iostreamnew{
         public:void show(){
             cout<<"\n class example\n";
         }
     }
     coutnew;
        
    }
    using namespace cybrom;
    int main() {
        bhopal::student();
        student();
        show();
        coutnew.show();
        iostreamnew obj;
        obj,show();
}